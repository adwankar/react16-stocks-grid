{
  "name": "stocks-grid",
  "version": "1.0.0",
  "description": "Stocks Grid showing capabilities of ExtReact 6.6 Grid",
  "repository": {
    "type": "git",
    "url": "https://github.com/adwankar/react16-stocks-grid"
  },
  "keywords": "React16, ExtReact6.6, Grid",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production webpack --env.prod=true --progress --colors",
    "clean": "rimraf build",
    "develop": "webpack --mode development",
    "pretest": "webpack --progress",
    "preupdate-snapshots": "webpack --progress",
    "prod": "cd build && static-server --port 8080",
    "start": "webpack-dev-server --progress --colors",
    "test": "jest",
    "update-snapshots": "jest -u"
  },
  "dependencies": {
    "@extjs/ext-react": "~6.6.0",
    "@extjs/ext-react-modern": "~6.6.0",
    "@extjs/ext-react-modern-theme-material": "~6.6.0",
    "@extjs/reactor16": "~2.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@extjs/reactor-babel-plugin": "~2.0.0",
    "@extjs/reactor-webpack-plugin": "~2.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.2.0",
    "open-browser-webpack-plugin": "0.0.5",
    "opn": "^5.3.0",
    "portfinder": "^1.0.13",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "static-server": "^2.2.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "./node_modules/@extjs/reactor/jest/setup.js",
      "./build/ext-react/ext.js"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ]
  }
}
